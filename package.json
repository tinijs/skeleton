{
  "name": "app",
  "version": "0.0.1",
  "description": "The base structure of a TiniJS app/lib.",
  "author": "Lam Nhan",
  "homepage": "https://tinijs.lamnhan.com",
  "license": "MIT",
  "main": "public-api.js",
  "typings": "public-api.d.ts",
  "files": [
    "app",
    "layouts",
    "pages",
    "components",
    "services",
    "styles",
    "public-api.js",
    "public-api.d.ts",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": ".tinijs/app/index.html",
  "staticFiles": {
    "staticPath": "public"
  },
  "targets": {
    "main": false
  },
  "scripts": {
    "compile": "tsc",
    "dev": "tini dev",
    "lint": "gts lint",
    "test": "tini test",
    "build": "tini build",
    "preview": "tini preview",
    "build:lib": "npm run compile",
    "ci": "npm run lint && npm run test && npm run build && npm run build:lib"
  },
  "dependencies": {
    "@tinijs/core": "file:../core/tinijs-core-0.0.1.tgz",
    "@tinijs/router": "file:../router/tinijs-router-0.0.1.tgz",
    "@tinijs/store": "file:../store/tinijs-store-0.0.1.tgz",
    "lit": "^2.5.0"
  },
  "devDependencies": {
    "@tinijs/cli": "file:../cli/tinijs-cli-0.0.1.tgz",
    "gts": "^4.0.0",
    "typescript": "~4.7.0"
  }
}
